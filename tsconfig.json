{
    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "node",
      "target": "ES2020",
      "outDir": "dist",
      "esModuleInterop": true,
      "strict": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "typeRoots": ["./node_modules/@types", "./src/types"],
    },
    "include": ["src"]
  }
  